            <string>/// Display the score calculation
var baseTreasure = global.runTreasure;
var multiplier = global.treasureMultiplier;
var totalTreasure = baseTreasure * multiplier;
var previousBank = global.bankedTreasure;
if (global.runAddedTreasure) {
    // We already changed global.bankedTreasure, so we have to undo the math here
    previousBank -= totalTreasure;
}
var newBank = previousBank + totalTreasure;

draw_text(window_get_width() / 2, 32, "Base treasure: " + string(baseTreasure));
draw_text(window_get_width() / 2, 32 + 32, "Multiplier: x" + string(multiplier));
draw_text(window_get_width() / 2, 32 + 64, "Total treasure: " + string(totalTreasure));
draw_text(window_get_width() / 2, 32 + 96, "Banked treasure: " + string(previousBank));
draw_text(window_get_width() / 2, 32 + 128, "New banked total: " + string(newBank));
draw_text(window_get_width() / 2, 32 + 192, "Press Enter to continue");

if (!global.runAddedTreasure) {
    global.bankedTreasure = newBank;
    global.runAddedTreasure = true;
    scriptSaveGame();
}

</string>
